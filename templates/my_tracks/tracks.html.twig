<turbo-frame id="my_page_top_tracks">
  <div>

    {% if pagination is defined and pagination == 1 %}
      {% set pagination = true %}
    {% else %}
      {% set pagination = false %}
    {% endif %}

    {% include 'base/html/table/table.html.twig' with {
      'thead' : myTracksThead,
      'tbody': tracks,
      'tbodyUrl' : myTracksTbodyUrl,
      'pagination' : pagination,
    } %}

  </div>
</turbo-frame>