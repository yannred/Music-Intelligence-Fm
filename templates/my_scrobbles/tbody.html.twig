<tbody>

{% for scrobble in tbody %}

  <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700">

    <td class="px-6 py-4">
      {% for image in scrobble.track.image %}
        {% if image.size == 1 %}
          <img src='{{ image.url }}' alt='{{ scrobble.track.name }}'>
        {% endif %}
      {% endfor %}
    </td>

    <th scope="row" class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">{{ scrobble.track.name }}</th>

    <td class="px-6 py-4">{{ scrobble.track.artist.name }}</td>

    <td class="px-6 py-4">{{ scrobble.track.album.name }}</td>

    <td class="px-6 py-4">{{ scrobble.timestamp|date('m/d/Y H:i:s') }}</td>

  </tr>

{% endfor %}

</tbody>